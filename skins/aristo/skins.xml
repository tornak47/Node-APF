<?xml version='1.0'?>
<a:skin xmlns:a="http://ajax.org/2005/aml" xmlns="http://www.w3.org/1999/xhtml">
<a:button name="button">
    <a:alias>submit</a:alias>
    <a:alias>reset</a:alias>
    
    <a:style><![CDATA[
        .aristo_btn {
            color       : #4f4f4f;
            font-family : Arial;
            font-size   : 12px;
            font-weight : bold;
            height      : 32px;
            width       : 100px;
            overflow    : hidden;
            cursor      : default;
        }
        
        .aristo_btn .left {
            float         : left;
            width         : 8px;
            height        : 32px;
            _margin-right : -3px; /* IE6 bug fix */
        }
        
        .aristo_btn .lbl {
            height        : 28px;
            padding-top   : 9px;
            text-align    : center;
            margin        : 0;
            _margin       : 0; /* IE6 fix */
            white-space   : nowrap;
            overflow      : hidden;
            text-overflow : ellipsis;
            text-shadow   : rgba(255, 255, 255, 0.8) 0px 1px 0;
        }
        
        .aristo_btn .right {
            float       : right;
            width       : 8px;
            height      : 32px;
           _margin-left : -3px; /* IE6 bug fix */
        }
        
        .aristo_btn .left,
        .aristo_btnDefault .left {
            background : url(images/button_borders.png) no-repeat 0 0;
        }
        
        .aristo_btn .lbl,
        .aristo_btnDefault .lbl {
            background : url(images/button_borders.png) repeat-x 0 -32px;
        }
        
        .aristo_btn .right,
        .aristo_btnDefault .right {
            background : url(images/button_borders.png) no-repeat 0 -64px;
        }
        
        .aristo_btn span {
            padding  : 1px 3px 1px 3px;
            border   : 1px solid transparent;
            position : relative;
            _border  : 0;
            _padding : 2px 4px 2px 4px;
            _margin  : 0;
        }
        
        .aristo_btnDisabled  {
            color : #a7a7a7;
        }
                
        .aristo_btnDisabled .left {
            background : url(images/button_borders.png) no-repeat 0 -288px;
        }
        
        .aristo_btnDisabled .lbl {
            background : url(images/button_borders.png) repeat-x 0 -320px;
        }
    
        .aristo_btnDisabled .right {
            background : url(images/button_borders.png) no-repeat 0 -352px;
        }
        
        .aristo_btnDown .left {
            background : url(images/button_borders.png) no-repeat 0 -192px;
        }
    
        .aristo_btnDown .lbl {
            background : url(images/button_borders.png) repeat-x 0 -224px;
        }
        
        .aristo_btnDown .right {
            background : url(images/button_borders.png) no-repeat 0 -256px;
        }

        .aristo_btnFocus .left,
        .aristo_btnOver .left {
            background : url(images/button_borders.png) no-repeat 0 -96px;
        }

        .aristo_btnFocus .lbl,
        .aristo_btnOver .lbl {
            background : url(images/button_borders.png) repeat-x 0 -128px;
        }

        .aristo_btnFocus .right,
        .aristo_btnOver .right {
            background : url(images/button_borders.png) no-repeat 0 -160px;
        }
    ]]></a:style>
    <a:style condition="apf.isIE7">
        .aristo_btn .right {
            margin : 0;
        }
        
        .aristo_btn .left {
            margin : 0;
        }
    </a:style>

    <a:presentation>
        <a:main caption="div[3]/span/text()" background=".">
            <div class="aristo_btn">
                <div class="left"> </div>
                <div class="right"> </div>
                <div class="lbl"><span>-</span></div>
             </div>
        </a:main>
    </a:presentation>
</a:button>

