    </a:presentation>
</a:slider>
<a:textbox name="textbox">
    <a:alias>input</a:alias>
    <a:alias>secret</a:alias>
    <a:style><![CDATA[
        .aristo_tb {
            position      : relative;
            background    : url(images/textfields.png) no-repeat 0 -90px;
            width         :  161px;
            height        : 30px;
            color         : #333;
            border        : 0;
            margin        : 0;
            padding       : 0 0 0 17px;
            outline       : none;
            font-family   : Arial;
            font-size     : 14px;
            text-overflow : ellipsis;
        }
        
        .aristo_tbFocus {
            background-position : 0 0;
        }
        
        .aristo_tbDisabled {
            color : #bebebe;
        }
        
        .aristo_tbError {
            border : 2px solid #ffb500;
            margin : -1px 0 0 -1px;
        }
        
        .aristo_tbInitial {
            color : gray;
        }

        .aristo_tb .aristo_tb_container {
            height     : 30px;
            background : url(images/textfields.png) repeat-x 0 -120px;
            padding    : 0;
            margin     : 0 17px 0 0;
        }

        .aristo_tbFocus .aristo_tb_container {
            background-position : 0 -30px;
        }

        .aristo_tb .aristo_tb_container input {
            border     : 0;
            background : transparent;
            width      : 100%;
            height     : 18px;
            margin     : 8px 0 0 0;
            padding    : 0;
        }

        .aristo_tb span {
            position   : absolute;
            background : url(images/textfields.png) no-repeat 0 -150px;
            width      : 17px;
            height     : 30px;
            top        : 0px;
            right      : 0px;
            margin     : 0;
            padding    : 0;
        }

        .aristo_tbFocus span {
            background-position : 0 -60px;
        }

        .aristo_tbautoc {
            border           : 1px solid gray;
            width            : 250px;
            overflow-y       : auto;
            z-index          : 10000000;
            position         : absolute;
            background-color : white;
            margin           : 23px 0 0 0;
            white-space      : nowrap;
            display          : none;
        }

        .aristo_tbautoc div {
            padding     : 1px 5px 1px 3px;
            cursor      : default;
            margin      : 0px;
            color       : black;
            font-family : Arial;
            font-size   : 9pt;
        }

        .aristo_tbautoc div.hover {
            background-color : #0B2B7A;
            color            : white;
        }
    ]]></a:style>
    <a:style condition="apf.isIE"><![CDATA[
        .aristo_tb .aristo_tb_container input {
            margin-top : 6px;
        }
    ]]></a:style>
    <a:presentation>
        <a:main input="div/input">
            <div class="aristo_tb">
                <div class="aristo_tb_container">
                    <input type="text" />
                </div>
                <span> </span>
            </div>
        </a:main>
        <a:container>
            <div class="aristo_tbautoc"> </div>
        </a:container>
        <a:item caption=".">
            <div>-</div>
        </a:item>
    </a:presentation>
</a:textbox>

<!--
    * Textarea doesn't scroll well -done
    * Textarea doesn't have a Focus state, should be similar to the textbox - done
-->
<a:textbox name="textarea">
    <a:style><![CDATA[
        .aristo_ta {
            border      : 1px solid #c3c3c3;
            background  : white url("images/textarea_background2.png") no-repeat top left;
            color       : #4b4b4b;
            padding     : 6px;
            font-family : Tahoma;
            font-size   : 8pt;
            overflow    : auto;
            outline     : none;
            resize      : none;
        }
        
        .aristo_taFocus {
            border-color : #327fbd;
        }

        .aristo_taDisabled {
            border : 1px solid #c3c3c3;
            color  : #bebebe;
        }
        
        .aristo_taInitial {
            color : gray;
        }

        .aristo_taError {
            border  : 2px solid #ffb500;
            padding : 5px;
        }
    ]]></a:style>

    <a:presentation>
        <a:main input=".">
            <textarea type='text' class="aristo_ta" />
        </a:main>
    </a:presentation>
</a:textbox>
