        .aristo_sliderVertDisabled {
        }

        .aristo_sliderVert .bottom {
            background : url(images/slider_corners.png) no-repeat 0 -21px;
            width      : 21px;
            height     : 3px;
            margin     : 0 0 -3px 0;
            padding    : 0;
            left       : 0;
            overflow   : hidden;
        }

        .aristo_sliderVertDisabled .left {
        }

        .aristo_sliderVert .grabber {
            background : url(images/sliders.png) no-repeat -21px -19px;
            width      : 22px;
            height     : 22px;
            overflow   : hidden;
            position   : absolute;
        }

        .aristo_sliderVertDisabled .grabber {
